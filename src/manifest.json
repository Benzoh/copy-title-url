{
  "name": "Get Title & URL",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Get the URL of the currently displayed page in markdown format.",
  "permissions": ["activeTab"],
  "browser_action": {
    "default_icon": {
      "19": "icon19.png"
    },
    "default_title": "Get Title & URL",
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "icon48.png"
  }
}